%table.table.table-striped.table-hover
	%thead
		%tr
			%th #
			%th Account
			%th Nickname
			%th Groups
	%tbody
		- @users.each_with_index do |user, index|
			%tr
				%td= (index + 1)
				%td= link_to user.account, groups_user_path(user), remote: true
				%td= user.nickname
				%td= group_label(user.group_titles)